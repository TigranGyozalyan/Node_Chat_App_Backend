{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off"
  }
}
